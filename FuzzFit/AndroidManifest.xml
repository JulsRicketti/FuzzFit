<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jfitnessfunctiontester"
    android:versionCode="1"
    android:versionName="1.0" >
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
	<uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/mr_fuzz_colored"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="activities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="activities.EnterActivityActivity"
            android:label="@string/title_activity_walking" >
        </activity>
        <activity
            android:name="activities.RunningActivity"
            android:label="@string/title_activity_running" >
        </activity>
        <activity
            android:name="activities.WeightLossActivity"
            android:label="@string/title_activity_weight_loss" >
        </activity>
        <activity
            android:name="activities.PedometerActivity"
            android:label="@string/title_activity_pedometer" >
        </activity>
        <activity
            android:name="activities.RegisterUserActivity"
            android:label="@string/title_activity_register_user" >
        </activity>
        <activity
            android:name="activities.ReportMenuActivity"
            android:label="@string/title_activity_report_menu" >
        </activity>
        <activity
            android:name="activities.ReportListViewActivity"
            android:label="@string/title_activity_report_list_view" >
        </activity>
        <activity
            android:name="com.example.jfitnessfunctiontester.ViewGraphActivity"
            android:label="@string/title_activity_view_graph" >
        </activity>
    </application>

</manifest>
